{
  "name": "lively.modules",
  "version": "0.2.2",
  "main": "dist/lively.modules.js",
  "scripts": {
    "test": "mocha-es6 tests/*-test.js",
    "build": "node tools/build.js"
  },
  "systemjs": {
    "main": "./index.js",
    "map": {
      "lively.modules": ".",
      "fetch": {
        "node": "@empty",
        "~node": "./node_modules/whatwg-fetch/fetch.js"
      },
      "fs": {
        "node": "@node/fs",
        "~node": "@empty"
      }
    }
  },
  "lively": {
    "main": "index.js",
    "packageMap": {
      "lively.lang": "./node_modules/lively.lang",
      "lively.ast": "./node_modules/lively.ast",
      "lively.vm": "./node_modules/lively.vm",
      "mocha-es6": "./node_modules/mocha-es6"
    },
    "meta": {
      "./node_modules/whatwg-fetch/fetch.js": {
        "format": "global",
        "exports": "fetch"
      }
    }
  },
  "dependencies": {
    "babel-core": "^5.8.38",
    "lively.ast": "^0.6.7",
    "lively.lang": "^0.5.22",
    "lively.vm": "^0.5.1",
    "systemjs": "^0.19.24",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "doc-comments": "^0.1.4",
    "mocha-es6": "^0.2.7",
    "rollup-plugin-babel": "^1.0.0"
  }
}
