  "dependencies": {
    "babel-core": "^6.21.0",
    "babel-plugin-transform-es2015-modules-systemjs": "^6.19.0",
    "babel-standalone": "^6.21.1-0",
    "lively.ast": "^0.9.0",
    "lively.lang": "^1.0.0",
    "lively.notifications": "^0.4.0",
    "lively.resources": "^0.1.2",
